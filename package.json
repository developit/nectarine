{
  "name": "nectarine",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline",
    "build": "mkdirp build && cross-env NODE_ENV=production webpack -p && cp -r src/assets build/",
    "prebuild": "rimraf build",
    "prestart": "npm run build -s",
    "start": "cross-var http-server -p ${PORT:-8080} -c-1 build",
    "deploy-old": "gh-pages -d build",
    "deploy": "npm run -s build && cp build/index.html build/200.html && surge -p build -d nectarine.rocks",
    "test": "eslint src"
  },
